<p id="notice"><%= notice %></p>

<h1>Repaire Parts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repaire_parts.each do |repaire_part| %>
      <tr>
        <td><%= link_to 'Show', repaire_part %></td>
        <td><%= link_to 'Edit', edit_repaire_part_path(repaire_part) %></td>
        <td><%= link_to 'Destroy', repaire_part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

