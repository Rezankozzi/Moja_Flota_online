<p id="notice"><%= notice %></p>

<h1>Venihles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venihles.each do |venihle| %>
      <tr>
        <td><%= link_to 'Show', venihle %></td>
        <td><%= link_to 'Edit', edit_venihle_path(venihle) %></td>
        <td><%= link_to 'Destroy', venihle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venihle', new_venihle_path %>
